{
  "name": "meme-soundboard",
  "version": "1.3.0",
  "private": true,
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "host": "serve dist/",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "debug": "vue ui",
    "refresh": "refresh",
    "bump": "yarn build && git add -f dist/* && git commit -m 'chore(build): add built files' && standard-version && git push --follow-tags origin master && firebase deploy",
    "deploy": "yarn build && firebase deploy && git checkout dist/",
    "start": "coffee -cbm ./ && yarn host"
  },
  "dependencies": {
    "@mdi/font": "5.1.45",
    "@vue/cli": "4.3.1",
    "commitizen": "4.0.4",
    "core-js": "3.6.5",
    "serve": "11.3.0",
    "vue": "2.6.11",
    "vuetify": "2.2.22"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@vue/cli-plugin-babel": "4.3.1",
    "@vue/cli-plugin-unit-jest": "4.3.1",
    "@vue/cli-plugin-vuex": "4.3.1",
    "@vue/cli-service": "4.3.1",
    "@vue/test-utils": "1.0.0-beta.33",
    "babel-loader": "8.1.0",
    "cache-loader": "4.1.0",
    "coffee-loader": "0.9.0",
    "coffeescript": "2.5.1",
    "css-loader": "3.5.2",
    "cz-conventional-changelog": "3.1.0",
    "husky": "4.2.5",
    "node-sass": "4.13.1",
    "sass": "1.26.3",
    "sass-loader": "8.0.2",
    "standard-version": "7.1.0",
    "vue-cli-plugin-coffee": "0.1.0",
    "vue-cli-plugin-vuetify": "2.0.5",
    "vue-loader": "15.9.2",
    "vue-template-compiler": "2.6.11",
    "vuetify-loader": "1.4.3",
    "webpack": "4.42.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
